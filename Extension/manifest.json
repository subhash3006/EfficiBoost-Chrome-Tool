{
    "manifest_version":3,
    "name":"EfficiBoost",
    "version":"0.0.1",
    "description":"A Extension used for blocking social media sites ",
    "browser_action":{
        "default_icon":"logo.png"
    },
    "icons":{
        "128":"logo.png"
    },
    "content_scripts":[
        {
            "matches":[
                "<all_urls>"
            ],
            "js":[
                "app.js"
            ]
        }
    ]
}
